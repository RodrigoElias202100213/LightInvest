@model LightInvest.Models.EducacaoEnergeticaViewModel

<h1>Educação Energética</h1>

<div>
    <h2>Artigos Educativos sobre Energias Renováveis</h2>
    <ul>
        @foreach (var artigo in Model.Artigos)
        {
            <li>
                <a href="@Url.Action("VerArtigo", "EducacaoEnergetica", new { id = artigo.Id })">
                    @artigo.Titulo
                </a>
            </li>
        }
    </ul>
</div>
